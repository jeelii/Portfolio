{
  "name": "full-stack-portfolio",
  "description": "Simple portfolio to showcase some projects and my skills. I use Webpack to bundle my assets.",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "babel-loader": "^9.1.3",
    "batch-image-resizer": "0.0.8",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "favicons": "^7.1.5",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^6.1.1",
    "handlebars": "^4.7.8",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "image-minimizer-webpack-plugin": "^4.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.8.0",
    "node-sass": "^9.0.0",
    "normalize.css": "^8.0.1",
    "pts": "^0.11.5",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  },
  "scripts": {
    "resize-images": "npm run image-resize-portfolio && npm run image-resize-medium && npm run image-resize-big",
    "image-resize-portfolio": "npx batch-image-resizer -i ./src/images/originals/portfolio -o ./src/images -s '[600]' -k",
    "image-resize-medium": "npx batch-image-resizer -i ./src/images/originals/medium -o ./src/images -s '[300]' -k",
    "image-resize-big": "npx batch-image-resizer -i ./src/images/originals/big -o ./src/images -s '[1700]' -k",
    "start": "webpack-dev-server  --watch-files ./src/partials/ --config webpack.dev.js --open",
    "add-domain": "echo \"jennysvensson.com\" > dist/CNAME",
    "build-cname": "webpack --config webpack.prod.js && npm run add-domain",
    "build": "webpack --config webpack.prod.js",
    "deploy": "gh-pages --dist 'dist' --branch 'release'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeelii/full-stack-portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeelii/full-stack-portfolio/issues"
  },
  "homepage": "https://github.com/jeelii/full-stack-portfolio#readme"
}
