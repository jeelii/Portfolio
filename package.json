{
  "name": "full-stack-portfolio",
  "description": "Simple portfolio to showcase some projects and my skills. I use Webpack to bundle my assets.",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@mdi/font": "^6.1.95",
    "@mdi/js": "^6.1.95",
    "babel-loader": "^8.2.2",
    "batch-image-resizer": "0.0.8",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.2.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^3.2.3",
    "handlebars": "^4.7.7",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.0",
    "mini-css-extract-plugin": "^2.2.2",
    "node-sass": "^6.0.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pts": "^0.10.6",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "resize-images": "npm run image-resize-portfolio && npm run image-resize-medium && npm run image-resize-big",
    "image-resize-portfolio": "npx batch-image-resizer -i ./src/images/originals/portfolio -o ./src/images -s '[600]' -k",
    "image-resize-medium": "npx batch-image-resizer -i ./src/images/originals/medium -o ./src/images -s '[300]' -k",
    "image-resize-big": "npx batch-image-resizer -i ./src/images/originals/big -o ./src/images -s '[1700]' -k",
    "start": "webpack-dev-server  --watch-files ./src/partials/ --config webpack.dev.js --open",
    "add-domain": "echo \"jennysvensson.com\" > dist/CNAME",
    "build": "webpack --config webpack.prod.js && npm run add-domain",
    "deploy": "gh-pages --dist 'dist' --branch 'release'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jennysvensson/full-stack-portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jennysvensson/full-stack-portfolio/issues"
  },
  "homepage": "https://github.com/jennysvensson/full-stack-portfolio#readme",
  "dependencies": {}
}
